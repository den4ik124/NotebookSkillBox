@using Notebook.Application.DTOs
@model NoteDto

@{
    ViewData["Title"] = "Note :" + @Model.Id;
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Note data</h1>


<table class="table">
    <thead>
        <tr>
            <th>
                ID
            </th>
            <th>
                Last name
            </th>
            <th>
                Name
            </th>
            <th>
                Third name
            </th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td>
                @Html.DisplayFor(modelItem => Model.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => Model.LastName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => Model.FirstName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => Model.ThirdName)
            </td>
           
        </tr>
    </tbody>
</table>
